{
  "name": "order-tracking-saas",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:webhook": "npx tsx --watch -r dotenv/config src/webhook-server/index.ts",
    "dev:payment": "npx tsx --watch -r dotenv/config src/workers/payment.worker.ts",
    "dev:email": "npx tsx --watch -r dotenv/config src/workers/email.worker.ts",
    "dev:tracking": "npx tsx --watch -r dotenv/config src/workers/tracking.worker.ts",
    "dev:payment-webhook": "npx tsx --watch -r dotenv/config src/workers/payment-webhook.worker.ts",
    "dev:lead": "npx tsx --watch -r dotenv/config src/workers/lead.worker.ts",
    "dev:mass-email": "npx tsx --watch -r dotenv/config src/workers/mass-email.worker.ts",
    "dev:ngrok": "ngrok http 3001",
    "test:email-flow": "npx tsx src/scripts/testEmailFlow.ts",
    "test:lead-email": "npx tsx src/scripts/test-lead-email.ts",
    "process:leads": "npx tsx src/scripts/process-leads.ts",
    "devall": "npx concurrently -k -n WEBHOOK,PAYMENT,EMAIL,TRACKING,PAYMENT-WH,MASS-EMAIL -c blue,green,yellow,magenta,cyan,red \"npm run dev:webhook\" \"npm run dev:payment\" \"npm run dev:email\" \"npm run dev:tracking\" \"npm run dev:payment-webhook\" \"npm run dev:mass-email\"",
    "dev": "vite",
    "dev:port": "vite --port 3001",
    "lint": "eslint .",
    "clean": "rimraf dist",
    "build:clean": "npm run clean && npm run build",
    "build": "npm run build:webhook && npm run build:workers",
    "build:frontend": "tsc -p tsconfig.frontend.json && vite build --config vite.config.frontend.cjs",
    "build:webhook": "scripts\\build-railway.bat",
    "build:workers": "tsc -p tsconfig.workers.json",
    "preview": "vite preview",
    "start": "node dist/server.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "setup:everything": "node scripts/setup-everything.js",
    "setup:simple": "node scripts/setup-simple.js",
    "setup:database": "node scripts/setup-database.js",
    "setup:auth": "node scripts/setup-complete-auth.js",
    "setup:gateway": "node scripts/setup-default-gateway.js",
    "setup:admin": "node scripts/create-admin-user.js",
    "monitor:workers": "node scripts/monitor-workers.bat",
    "monitor:workers:loop": "node -e \"setInterval(() => { require('child_process').exec('npm run monitor:workers', (err) => { if(err) console.error('Erro no monitoramento:', err); }); }, 300000);\"",
    "devall:monitored": "npm run devall & npm run monitor:workers:loop",
    "fix:address-validation": "node scripts/fix-address-validation.js"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.787.0",
    "@headlessui/react": "^1.7.19",
    "@netlify/functions": "^2.6.0",
    "@react-email/components": "^0.0.36",
    "@react-email/render": "^1.0.6",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.10",
    "@tanstack/react-query": "^5.28.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ws": "^8.18.1",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.4.6",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "framer-motion": "^12.9.2",
    "handlebars": "^4.7.8",
    "html-pdf-node": "^1.0.8",
    "html2canvas": "^1.4.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.344.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.9.12",
    "puppeteer": "^24.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2",
    "redis": "^4.6.13",
    "resend": "^4.4.1",
    "tailwind-merge": "^2.2.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.15.0",
    "@types/pg": "^8.11.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.0",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
